# Eclipse/STS/IntelliJ IDE metadata
.vscode/               # VS Code settings
*.launch               # Launch configurations
*.ipr                  # IntelliJ project files
*.iws                  # IntelliJ workspace files
*.dbeaver/             # DBeaver settings
*.factorypath          # Eclipse plugin metadata
.mvn/                  # Maven wrapper files

# Java-related
*.jar                  # Java archives
*.war                  # Web archives
*.ear                  # Enterprise archives
*.javadoc              # Java documentation
*.class                # Compiled Java classes
*.aj                  # AspectJ files

# Gradle
.gradle/               # Gradle cache
build/                 # Gradle build output
!gradle-wrapper.jar    # Include wrapper jar

# Maven
target/                # Maven build output
pom.xml.tag
pom.xml.releaseBackup
pom.xml.versionsBackup
pom.xml.next
release.properties

# Logs and Reports
*.log                  # Log files
*.out                  # Output logs
*.report               # Report files
*.bak                  # Backup files
*.tmp                  # Temporary files
hs_err_pid*            # Java crash logs

# System and Editor Files
*.swp                  # Swap files
*.swo                  # Swap files
*.orig                 # Merge conflict backup
.idea/                 # IntelliJ project settings
*.iml                  # IntelliJ module files

# macOS specific
.AppleDouble
.LSOverride

